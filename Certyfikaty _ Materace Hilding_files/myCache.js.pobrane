/**
 * obs≈Çuga cache - chwilowe np przy sliderach pobieranych ajax-ow
 */
function My_Cache(){
    var cache = new Object();
    
    this.set = function( id, value ){
        cache[id] = value ;
    }
    
    this.get = function( id ){
        if( cache[id] == undefined ) return false;
        
        return cache[id];
    }
}